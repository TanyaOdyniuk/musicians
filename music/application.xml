<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="artist" class="entity.Artist" scope="prototype">
              <property name="name" value="Matthew"></property>
              <property name="surname" value="Bellamy"></property>
              <property name="instrumentList">
                     <list>
                            <ref bean="guitar"></ref>
                            <ref bean="drums"></ref>
                     </list>
              </property>

       </bean>

       <bean id="guitar" class="entity.Instrument">
              <property name="name" value="Guitar"></property>
       </bean>

       <bean id="drums" class="entity.Instrument">
              <property name="name" value="Drums"></property>
       </bean>

</beans>